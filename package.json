{
  "name": "aimless.js",
  "version": "1.0.2",
  "description": "The missing JS randomness library.",
  "keywords": [
    "aimless",
    "aimless.js",
    "random",
    "js random",
    "range",
    "sequence",
    "weighted",
    "distribution",
    "seeded"
  ],
  "repository": "ChrisCavs/aimless.js",
  "license": "MIT",
  "author": {
    "name": "Chris Cavalea",
    "email": "chriscavs94@gmail.com",
    "url": "https://chriscavs.github.io/"
  },
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./package.json": "./package.json"
  },
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "source": "src/index.ts",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "npx tsup",
    "dev": "npx tsup --watch",
    "prepublish": "npx vitest run",
    "server": "npx serve",
    "pretest": "npm run build",
    "test": "npx vitest"
  },
  "devDependencies": {
    "serve": "^14.2.0",
    "ts-standard": "^12.0.2",
    "tsup": "^6.7.0",
    "vitest": "^0.31.4"
  }
}
